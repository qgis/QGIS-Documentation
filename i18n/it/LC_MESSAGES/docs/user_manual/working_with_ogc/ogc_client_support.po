msgstr "Inserire il nome 'DM Solutions'"

# 970eb6e6ee9a459d8716ef3aebe7bf55
#: ../../source/docs/user_manual/working_with_ogc/ogc_client_support.rst:545
msgid "Enter the URL (see above)"
msgstr "Inserire l’indirizzo precedentemente indicato"

# 4afaae1c28c54b72903bed20eda9bd87
#: ../../source/docs/user_manual/working_with_ogc/ogc_client_support.rst:546
msgid "Click **[OK]**"
msgstr "Cliccare su **[OK]**"

# adc38f3158c848bbb91eb894290b2e01
#: ../../source/docs/user_manual/working_with_ogc/ogc_client_support.rst:549
