msgid ""
"An example of this type of address is http://opencache.statkart.no/"
"gatekeeper/gk/gk.open_wmts?service=WMTS&request=GetCapabilities"
msgstr ""

# 17b1d0e17d2c43129771d66c952174ac
#: ../../source/docs/user_manual/working_with_ogc/ogc_client_support.rst:125
msgid ""
"For testing the topo2 layer in this WMTS works nicely. Adding this string "
"indicates that a WMTS web service is to be used instead of a WMS service"
msgstr ""

# d9b8a251f5044b7b8d3d9cabf5c50035
#: ../../source/docs/user_manual/working_with_ogc/ogc_client_support.rst:128
msgid ""
