msgid "Options:"
msgstr ""

#: ../../source/docs/user_manual/processing_algs/taudem/specialized_grid_analysis_tools.rst:482
#: ../../source/docs/user_manual/processing_algs/taudem/specialized_grid_analysis_tools.rst:590
msgid "0 --- Minimum"
msgstr ""

#: ../../source/docs/user_manual/processing_algs/taudem/specialized_grid_analysis_tools.rst:483
#: ../../source/docs/user_manual/processing_algs/taudem/specialized_grid_analysis_tools.rst:591
msgid "1 --- Maximum"
msgstr ""

