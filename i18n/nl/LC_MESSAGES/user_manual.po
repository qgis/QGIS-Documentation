#: ../../source/user_manual/working_with_ogc/ogc_client_support.rst:404
msgid "WMS Client Limitations"
msgstr ""

# 7a7feae9b4ed4e8cb523d02898691bae
#: ../../source/user_manual/working_with_ogc/ogc_client_support.rst:407
