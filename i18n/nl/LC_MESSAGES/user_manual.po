msgstr ""

# a08d450bbe6e449d8ab529d9cdec3b13
#: ../../source/user_manual/working_with_vector/vector_properties.rst:211
msgid "Categorized Renderer"
msgstr ""

# b6e98ae5c1074f5f9c4c61be30dc0553
#: ../../source/user_manual/working_with_vector/vector_properties.rst:214
msgid ""
"The Categorized Renderer is used to render all features from a layer, using "
"a single user-defined symbol, which color reflects the value of a selected "
"feature's attribute. The :guilabel:`Style` tab allows you to select:"
msgstr ""

# 4e1f8f23540a46a191050287ebc3f24d
# 1a33b04776a1447481429bc54b515be4
#: ../../source/user_manual/working_with_vector/vector_properties.rst:219
#: ../../source/user_manual/working_with_vector/vector_properties.rst:270
msgid "The attribute (using the Column listbox)"
msgstr ""

# 6cb943611e8541fbb6b157f191e38669
#: ../../source/user_manual/working_with_vector/vector_properties.rst:220
msgid "The symbol (using the Symbol dialog)"
msgstr ""

# 4d539a67eafe421981e5dea6a5b175a4
#: ../../source/user_manual/working_with_vector/vector_properties.rst:221
