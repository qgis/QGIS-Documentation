\newcommand*\bt@@item[2][]{%
    \ifthenelse{\equal{#2}{}}{%
        \def\endbt@item{\relax}%
    }{%
        \ifthenelse{\equal{#1}{}}{%
            \gdef\bt@args{}%
        }{%
            \bt@isnum{#1}{%
                \ifthenelse{\boolean{bt@override@numargs}}{%
                    \global\bt@overridden@numargstrue
                    \gdef\bt@args{}%
                }{%
                    \bt@warn@brackets{#2}#1[]\end%
                    \protected@xdef\bt@args{[#1]}%
                }%
            }{%
                \bt@warn@brackets{#2}#1[]\end%
                \protected@xdef\bt@args{[#1]}%
            }%
        }%
        \bt@call@item{#2}%
    }%
}
\newcommand*\bt@warn@brackets{}
