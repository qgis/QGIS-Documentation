\newcommand*\bt@@harvitem[4][]{%
    \ifthenelse{\equal{#1}{}}{%
        \protected@xdef\bt@args{[#2(#3)]}%
    }{%
        \bt@warn@brackets{#2}#1[]\end%
        \protected@xdef\bt@args{[#1(#3)#2]}%
