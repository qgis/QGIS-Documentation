\newcommand*\bt@if@files@differ[4]{%    
    \bt@files@difffalse
    \def\bt@line@cnt{0}%
    \bgroup
        \let\do\@makeother
        \dospecials
        \endlinechar-1
        \openin\bt@infilea=#1
        \openin\bt@infileb=#2
        \bt@conttrue
        \loop
        \ifeof\bt@infilea
            \bt@contfalse
            \ifeof\bt@infileb
            \else
