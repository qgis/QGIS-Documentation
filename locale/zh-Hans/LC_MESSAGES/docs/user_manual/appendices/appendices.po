#: ../../docs/user_manual/appendices/appendices.rst:809
msgid "project title"
msgstr ""

#: ../../docs/user_manual/appendices/appendices.rst:810
msgid "project CRS"
msgstr ""

#: ../../docs/user_manual/appendices/appendices.rst:811
msgid "the layer tree"
msgstr ""

#: ../../docs/user_manual/appendices/appendices.rst:812
msgid "snapping settings"
msgstr ""

#: ../../docs/user_manual/appendices/appendices.rst:813
msgid "relations"
msgstr ""

#: ../../docs/user_manual/appendices/appendices.rst:814
msgid "the map canvas extent"
msgstr ""

#: ../../docs/user_manual/appendices/appendices.rst:815
msgid "project models"
msgstr ""

#: ../../docs/user_manual/appendices/appendices.rst:816
msgid "legend"
msgstr ""

#: ../../docs/user_manual/appendices/appendices.rst:817
msgid "mapview docks (2D and 3D)"
msgstr ""

#: ../../docs/user_manual/appendices/appendices.rst:818
msgid ""
"the layers with links to the underlying datasets (data sources) and other "
"layer properties including extent, SRS, joins, styles, renderer, blend mode,"
" opacity and more."
msgstr ""

#: ../../docs/user_manual/appendices/appendices.rst:821
msgid "project properties"
msgstr ""

