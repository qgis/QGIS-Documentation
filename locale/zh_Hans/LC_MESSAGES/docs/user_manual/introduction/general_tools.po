msgid "selectRectangle"
msgstr ""

#: ../../docs/user_manual/introduction/general_tools.rst:839
msgid "|selectPolygon| :sup:`Select Features by Polygon`"
msgstr "|selectPolygon| :sup:`按多边形选择要素`"

#: ../../docs/user_manual/introduction/general_tools.rst:2301
msgid "selectPolygon"
msgstr ""

#: ../../docs/user_manual/introduction/general_tools.rst:840
msgid "|selectFreehand| :sup:`Select Features by Freehand`"
msgstr "|selectFreehand| :sup:`自由手绘选择要素`"

#: ../../docs/user_manual/introduction/general_tools.rst:2297
msgid "selectFreehand"
msgstr ""

#: ../../docs/user_manual/introduction/general_tools.rst:841
