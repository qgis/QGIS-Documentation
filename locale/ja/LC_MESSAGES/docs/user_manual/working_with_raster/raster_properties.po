msgid "Gauss"
msgstr "ガウス"

#: ../../docs/user_manual/working_with_raster/raster_properties.rst:808
msgid "Cubic"
msgstr "キュービック"

#: ../../docs/user_manual/working_with_raster/raster_properties.rst:809
msgid "Cubic Spline"
msgstr "キュービック・スプライン"

#: ../../docs/user_manual/working_with_raster/raster_properties.rst:810
msgid "Laczos"
msgstr "ランチョス法"

