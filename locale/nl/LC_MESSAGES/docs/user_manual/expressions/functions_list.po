msgstr ""
"``regexp_replace('QGIS SHOULD ROCK','\\\\sSHOULD\\\\s',' DOES ')`` â 'QGIS "
"DOES ROCK'"

#: ../../docs/user_manual/expressions/expression_help/String.rst:43
msgid "``regexp_replace('ABC123','\\\\d+','')`` â 'ABC'"
msgstr "``regexp_replace('ABC123','\\\\d+','')`` â 'ABC'"

