msgstr "De omgevingsvariabele ``QGIS_SERVER_<service>_SERVICE_URL``"

#: ../../docs/server_manual/config.rst:249
msgid "The ``QGIS_SERVER_SERVICE_URL`` environment variable"
msgstr "De omgevingsvariabele ``QGIS_SERVER_SERVICE_URL``"

#: ../../docs/server_manual/config.rst:250
msgid "The ``X-Qgis-<service>-Service-Url`` header"
msgstr "De header ``X-Qgis-<service>-Service-Url``"

#: ../../docs/server_manual/config.rst:251
msgid "The ``X-Qgis-Service-Url`` header"
msgstr "De header ``X-Qgis-Service-Url``"

#: ../../docs/server_manual/config.rst:252
msgid "Build from the ``Forwarded`` header"
msgstr "Gebouwd vanuit de header ``Forwarded``"

#: ../../docs/server_manual/config.rst:253
msgid "Build from the ``X-Forwarded-Host`` and ``X-Forwarded-Proto`` headers"
msgstr ""
