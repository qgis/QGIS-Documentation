msgid "``concat('a','b','c','d','e')`` → 'abcde'"
msgstr "``concat('a','b','c','d','e')`` → 'abcde'"

#: ../../docs/user_manual/expressions/expression_help/String.rst:66
msgid "``concat('Anno ', 1984)`` → 'Anno 1984'"
msgstr "``concat('Anno ', 1984)`` → 'Anno 1984'"

#: ../../docs/user_manual/expressions/expression_help/String.rst:67
