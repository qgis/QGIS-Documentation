#: ../../docs/user_manual/expressions/expression_help/GeometryGroup.rst:35
msgid ""
"``geom_to_wkt(affine_transform(geom_from_wkt('LINESTRING(1 1, 2 2)'), 2, 2, "
"0, 1, 1))`` → 'LineString (3 3, 4 4)'"
msgstr ""
"``geom_to_wkt(affine_transform(geom_from_wkt('LINESTRING(1 1, 2 2)'), 2, 2, "
"0, 1, 1))`` → 'LineString (3 3, 4 4)'"

#: ../../docs/user_manual/expressions/expression_help/GeometryGroup.rst:36
msgid ""
"``geom_to_wkt(affine_transform(geom_from_wkt('POLYGON((0 0, 0 3, 2 2, 0 "
"0))'), 0, 0, -90, 1, 2))`` → 'Polygon ((0 0, 6 0, 4 -2, 0 0))'"
msgstr ""
