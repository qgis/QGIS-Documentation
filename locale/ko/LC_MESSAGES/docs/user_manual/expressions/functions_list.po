msgstr "``substr('HELLO',3,-1)`` → 'LL'"

#: ../../docs/user_manual/expressions/expression_help/String.rst:48
msgid "``substr('HELLO WORLD',-5,2)`` → 'WO'"
msgstr "``substr('HELLO WORLD',-5,2)`` → 'WO'"

#: ../../docs/user_manual/expressions/expression_help/String.rst:49
msgid "``substr('HELLO WORLD',-5,-1)`` → 'WORL'"
msgstr "``substr('HELLO WORLD',-5,-1)`` → 'WORL'"

