msgstr ":guilabel:`Sort by Name`: 이름으로 정렬"

#: ../../docs/user_manual/plugins/plugins.rst:184
msgid ":guilabel:`Sort by Downloads`"
msgstr ":guilabel:`Sort by Downloads`: 다운로드로 정렬"

#: ../../docs/user_manual/plugins/plugins.rst:185
msgid ":guilabel:`Sort by Vote`"
msgstr ":guilabel:`Sort by Vote`: 평점으로 정렬"

#: ../../docs/user_manual/plugins/plugins.rst:186
msgid ":guilabel:`Sort by Status`"
msgstr ":guilabel:`Sort by Status`: 설치 및 업그레이드 상태로 정렬"

#: ../../docs/user_manual/plugins/plugins.rst:187
msgid ":guilabel:`Sort by Date Created`"
msgstr ":guilabel:`Sort by Date Created`: 생성 날짜로 정렬"

#: ../../docs/user_manual/plugins/plugins.rst:188
msgid ":guilabel:`Sort by Date Updated`"
msgstr ":guilabel:`Sort by Date Updated`: 업데이트 날짜로 정렬"

