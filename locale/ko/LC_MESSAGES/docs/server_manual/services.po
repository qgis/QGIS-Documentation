#: ../../docs/server_manual/services.rst:906
#: ../../docs/server_manual/services.rst:1438
msgid "``image/jpeg``"
msgstr "``image/jpeg``"

#: ../../docs/server_manual/services.rst:355
#: ../../docs/server_manual/services.rst:908
#: ../../docs/server_manual/services.rst:1439
msgid "``image/png``"
msgstr "``image/png``"

#: ../../docs/server_manual/services.rst:356
